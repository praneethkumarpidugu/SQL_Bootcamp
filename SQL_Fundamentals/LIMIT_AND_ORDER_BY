SELECT *
FROM customer LIMIT 10;

SELECT first_name,
		last_name
FROM
	customer
ORDER BY first_name;

SELECT first_name,
		last_name
FROM
	customer
ORDER BY last_name DESC;

SELECT first_name,
		last_name
FROM
	customer
ORDER BY first_name ASC,
		last_name DESC;
        
SELECT first_name,
		last_name
FROM
	customer
ORDER BY first_name ASC,
		last_name ASC;

